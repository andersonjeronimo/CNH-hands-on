<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="container">

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main>

        <div class="container">           

            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Nome</th>
                        <th scope="col">Telefone</th>
                        <th scope="col">CPF</th>
                        <th scope="col">Status</th>
                    </tr>
                </thead>
                <tbody>
                    <% items.forEach(item=> { %>
                        <tr>
                            <th scope="row">
                                <%= item.id %>
                            </th>                            
                            <td>
                                <%= item.name %>
                            </td>
                            <td>
                                <%= item.phone %>
                            </td>
                            <td>
                                <%= item.cpf %>
                            </td>
                            <td>
                                <%= item.status %>
                            </td>
                        </tr>
                        <% }) %>

                </tbody>
            </table>

        </div>

    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>

</html>