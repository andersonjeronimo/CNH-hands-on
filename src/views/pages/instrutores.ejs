<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="container">

    <!-- <header>
        <%- include('../partials/header'); %>
    </header> -->

    <main>

        <div class="container">     
            <hr>
            <img src="/images/logo-glass-2.png" class="img-fluid shadow bg-white rounded" alt="CNH na mao logo com lupa" width="50%">
            <hr>

            <table class="table table-striped">
                <thead>
                    <tr>                        
                        <th scope="col">Nome</th>
                        <th scope="col">Email</th>
                        <th scope="col">Telefone</th>
                        <th scope="col">CPF</th>
                        <th scope="col">Status</th>
                        <th scope="col">Categoria</th>
                        <th scope="col">Ve√≠culo</th>
                        <th scope="col">Estado</th>
                        <th scope="col">Cidade</th>
                    </tr>
                </thead>
                <tbody>
                    <% items.forEach(item=> { %>
                        <tr>
                            <th scope="row">
                                <%=  item.lastname%> , <%= item.firstname %>
                            </th>                            
                            <td>
                                <%= item.email %>
                            </td>
                            <td>
                                (<%= item.ddd%>)<%=item.phone %>
                            </td>
                            <td>
                                <%= item.cpf %>
                            </td>
                            <td>
                                <%= item.status %>
                            </td>
                            <td>
                                <%= item.category %>
                            </td>
                            <td>
                                <%= item.vehicle %>
                            </td>
                            <td>
                                <%= item.state %>
                            </td>
                            <td>
                                <%= item.city %>
                            </td>
                        </tr>
                        <% }) %>

                </tbody>
            </table>

        </div>

    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>

</html>